<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mayday.client.service.IClientDao">
	<select id="getClientList" resultType="com.mayday.client.vo.ClientVO">
		SELECT user_id
		     , user_pass
		     , user_phone
		FROM client
